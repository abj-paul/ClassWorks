digraph DFD {
	user [label=User color=lightblue shape=rectangle style=filled]
	main [label=Main color=yellow shape=ellipse style=filled]
	list_files [label="List Files" color=yellow shape=ellipse style=filled]
	search_file [label="Search File" color=yellow shape=ellipse style=filled]
	copy_file [label="Copy File" color=yellow shape=ellipse style=filled]
	delete_file [label="Delete File" color=yellow shape=ellipse style=filled]
	log [label=Log color=yellow shape=ellipse style=filled]
	file_system [label="File System" color=lightgreen shape=cylinder style=filled]
	log [label=Log color=lightgreen shape=cylinder style=filled]
	main -> list_files [label=Call]
	main -> search_file [label=Call]
	main -> copy_file [label=Call]
	main -> delete_file [label=Call]
	main -> log [label="Log Entry"]
	main -> file_system [label="File Interaction"]
	user -> list_files [label=directory]
	list_files -> log [label=Call]
	list_files -> log [label=Call]
	list_files -> log [label="Log Entry"]
	list_files -> file_system [label="File Interaction"]
	user -> search_file [label=directory]
	user -> search_file [label=filename]
	search_file -> log [label=Call]
	search_file -> log [label=Call]
	search_file -> log [label=Call]
	search_file -> log [label="Log Entry"]
	search_file -> file_system [label="File Interaction"]
	user -> copy_file [label=source]
	user -> copy_file [label=destination]
	copy_file -> log [label=Call]
	copy_file -> log [label=Call]
	copy_file -> log [label="Log Entry"]
	copy_file -> file_system [label="File Interaction"]
	user -> delete_file [label=filepath]
	delete_file -> log [label=Call]
	delete_file -> log [label=Call]
	delete_file -> log [label="Log Entry"]
	delete_file -> file_system [label="File Interaction"]
	user -> log [label=message]
	log -> log [label="Log Entry"]
	log -> file_system [label="File Interaction"]
}
